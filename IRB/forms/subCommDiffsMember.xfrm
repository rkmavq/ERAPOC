    <script ev:event="xforms-model-construct-done"> 
      bootstrapSharedXfrm(model);

      model.Frm = new appForm(model, "docs");
      
      includeSharedJS("/resources/applications/IRB/js/clientSideFunctions.js");
      model.USA = new irbProtocol(model);
      resX=callSharedService("changeAgg",{op:"fixRevIndexes",testUri: model.Frm.linkedOpURI + model.Frm.formSet.getRoot().__COMPREVURI}, "json");              
      resX=callSharedService("changeAgg",{op:"genChanges",testUri:model.Frm.linkedOpURI + model.Frm.formSet.getRoot().__COMPREVURI, lop:model.Frm.linkedOpType}, "json");      
      //resX=callSharedService("protocolRouting",{op:"generateReviewHistoryDynamic",linkedOp:"",linkedOpURI:model.Frm.linkedOpURI,baseURI:model.Frm.targetURI,revDataURI:model.Frm.linkedOpURI + model.Frm.formSet.getRoot().__COMPREVURI}, "json");
      model.Frm.loadOpFile('dat', model.Frm.formSet.getRoot().__COMPREVURI, "0");
      model.Frm.reloadInst('dat');
if(model.Frm.formSet.getRoot().__AMND){
      
      model.Frm.loadOpFile('proReviews', "/reviews.xml", "0");
      
      }else{
      if(model.Frm.linkedOpURI!=''){
      model.Frm.loadOpFile('proReviews', "/reviews.xml", "0");
      }else{      
      model.Frm.loadAppFile('proReviews', "/reviews.xml", "0");
      }
      }
      model.Frm.loadAppFile('cats', "/data/catagories.xml", "0");
      
      //AB: get last review that was of type res and review num is less than current one and last one
      lastRSRevNum = model.getValue("instance('proReviews')/review[@num&lt;"+model.Frm.formSet.getRoot().model.TS.model.getValue("instance('ins')/reviewID")+" and @def='res'][last()]/@num");
      lastRSRevNumParent = model.getValue("instance('proReviews')/review[@num&lt;"+model.Frm.formSet.getRoot().model.TS.model.getValue("instance('ins')/reviewID")+" and @def='res'][last()]/@parent");
      model.setValue("instance('form')/revNum", model.getValue("instance('proReviews')/review[@num&lt;"+model.Frm.formSet.getRoot().model.TS.model.getValue("instance('ins')/reviewID")+" and @def='res'][last()]/@num"));
      model.setValue("instance('form')/metaNum", model.getValue("instance('proReviews')/metaReview[@num&lt;"+lastRSRevNumParent+"]/@num"));
      //AB 02/19/2019 solved for issue where proper basechange was not getting set when you tab into comments from personnel tab. 
      //below we also check to make sure basechange is loded only when its an amendment.
      //if(model.Frm.formSet.getRoot().__AMND){  
      if(model.Frm.formSet.getRoot().__AMND &amp;&amp; model.Frm.formSet.getRoot().__AMND!="undefined" &amp;&amp; model.Frm.linkedOpType=="amendments"){   
      model.setInstanceIdSrc('diffs', "/fil/ideate/configs/IRB/forms/baseChange.xml");
      }else{
      model.setInstanceIdSrc('diffs', "/fil/ideate/configs/IRB/forms/" + model.Frm.linkedOpType + "BaseChange.xml");
      }
    </script>
