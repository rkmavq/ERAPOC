<?xml version="1.0" encoding="UTF-8"?>
<naming-scheme>
    <format>PRE-YYYY-#</format> <!-- Either:  #, PRE-# (default),  PRE-YY-#, PRE-YYYY-#, PRE-YYYY-MM-#, PRE-YY-MM-#, YY-#, YYYY-#, YYYY-MM-#, YY-MM-# -->
    <year>calendar</year>       <!-- Either: calendar (default), fiscal (determines year to quote if YY or YYYY is selected above)-->
    <fiscalMonth>06</fiscalMonth> <!-- If the above is fiscal, the ## (make sure the leading 0 is include!) of the month where the pivot occurs -->
    <pre-fix>OTEU</pre-fix>     <!-- Pad added to the front of each number (If PRE- above)-->
    <incriment>1</incriment>    <!-- Increment ammount, next number = last number + increment -->
    <digits>4</digits>          <!-- If set, determines the minimum # of digits that should be displayed (adds leading zeros) -->
    <lastNum>96</lastNum>      <!-- Determins the next number in the sequence -->
    <lastYear>2011</lastYear>
</naming-scheme>