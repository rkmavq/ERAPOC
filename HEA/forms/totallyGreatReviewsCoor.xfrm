    							<exf:variable name="expert" value="expertise"/>              		
    							<xf:repeat nodeset="instance('cats')/boardMemberExpertise/type[contains($expert, @id)]">
    								<xf:output value="concat(if(position()=1, '', ', '), @name)"/>  
