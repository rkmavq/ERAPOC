    			<u style="color:blue;cursor:pointer;text-decoration:underline;">
    				Add Selected Members
    				<xf:action ev:event="click">
    					<script> 
    						model.USA.addReviewers();
    					</script>               
    				</xf:action>
    				<xf:close/>
    			</u>
